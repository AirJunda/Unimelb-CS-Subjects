f <- ggplot(data.frame(x = c(0.00001, 1)), aes(x, color="pink", size=2))
test <- function(x) {(1/36)*exp(-((36*x-36/2.71828182845905)^4))-3*x*log10(x)}
f <- f + stat_function(fun = test)+theme(legend.position="none") +
coord_flip()
f
